<h1>Parents#index</h1>
<p><%=link_to "学校ユーザー作成", new_parent_path %></p>
<% @parents.each do |parent| %>
<p><%=link_to parent.name, parent_path(parent)%></p>
<p><%=link_to "編集", edit_parent_path(parent) %></p>
<p><%=link_to "削除", parent_path(parent), method: :delete, data:{confirm: '本当に削除しますか？'} %></p>
<% end %>
